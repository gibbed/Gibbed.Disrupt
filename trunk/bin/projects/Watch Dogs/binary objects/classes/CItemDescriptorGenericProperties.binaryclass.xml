<class name="CItemDescriptorGenericProperties">
	<inherit name="IItemDescriptorProperties" />
	<object name="ItemAvailability">
		<inherit name="CAvailabilityRuleGroup" />
	</object>
	<field name="selEquippableType" type="Enum8">
		<enum name="enumEquippableType">
			<element name="Not_Equippable">-1</element>
			<element name="One_Handed">0</element>
			<element name="Assault">1</element>
			<element name="Shotgun">2</element>
			<element name="Sniper">3</element>
			<element name="Projectiles">4</element>
			<element name="Ar_OneHanded">5</element>
			<element name="Ar_Assault">6</element>
			<element name="Ar_SingleShotRifle">7</element>
			<element name="Ar_ShotGun">8</element>
			<element name="Ar_Uzi">9</element>
			<element name="Ar_Replica">10</element>
		</enum>
	</field>
	<object name="locstridItemNameId" desc="Localized string ID to use">
		<inherit name="LocalizationId" />
	</object>
	<object name="locstridItemDescriptionId">
		<inherit name="LocalizationId" />
	</object>
	<object name="locstridItemLongDescriptionId">
		<inherit name="LocalizationId" />
	</object>
	<field name="uiItemMaxNbToOwn" type="Int8" min="1" max="999999" />
	<field name="uiItemNbToAdd" type="Int8" />
	<field name="hackingparametersHackingConfigOverride" type="BinHex" />
	<field name="selQualityLevelType" type="Enum8">
		<enum name="enumQualityLevelType">
			<element name="None">0</element>
			<element name="Average">1</element>
			<element name="Good">2</element>
			<element name="VeryGood">3</element>
		</enum>
	</field>
	<field name="accessidAccessIdToGiveItem" desc="Item will be given to the player when the accessId is acquire/unlocked by the player" type="BinHex"></field>
	<field name="bAutoGiveItemFromAccessId" desc="Auto give the item into player',27h,'s inventory using the accessIdToGive." type="Boolean" />
</class>